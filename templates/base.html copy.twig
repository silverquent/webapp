		<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDc6UOlmBmZ-U6b1Arh6Pny0DFdZubM3Cc&libraries=places"></script>
		{% block stylesheets %}

			{{ encore_entry_link_tags('app') }}

		{% endblock %}
	</head>
	<body class="hold-transition sidebar-mini layout-fixed">
		<div
			class="wrapper">

			<!-- Navbar -->
			<nav
				class="main-header navbar navbar-expand navbar-white navbar-light">
				<!-- Left navbar links -->
				<ul class="navbar-nav">

					<li class="nav-item">
						<a class="nav-link" data-widget="pushmenu" href="#" role="button">
							<i class="fas fa-bars"></i>
						</a>
					</li>
					{% if is_granted('ROLE_SUPER_ADMIN') %}
						<li class="nav-item d-none d-sm-inline-block">
							<a href="{{path('dashboard')}}" class="nav-link">Acceuil</a>
						</li>
						<li class="nav-item d-none d-sm-inline-block">
							<a href="#" class="nav-link">Contact</a>
						</li>
						{% endif %}
						<li class="nav-item d-none d-sm-inline-block">
							<a href="{{path('app_logout')}}" class="nav-link">DÃ©connexion</a>
						</li>
					
				</ul>

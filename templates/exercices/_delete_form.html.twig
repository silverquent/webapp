<form class='noform' method="post" action="{{ path('app_exercices_delete', {'id': exercice.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ exercice.id) }}">
	<button class="btn btn-danger ml-1">
		<i class="fa fa-trash"></i>
	</button>
</form>
